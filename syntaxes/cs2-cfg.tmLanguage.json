{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "CS2 Config",
	"patterns": [
		{ "include": "#comments" },
		{ "include": "#commands" },
		{ "include": "#strings" },
		{ "include": "#network" },
		{ "include": "#values" },
		{ "include": "#separators" }
	],
	"repository": {
		"comments": {
			"patterns": [
				{
					"name": "comment.line.double-slash.cs2-cfg",
					"match": "//.*$"
				}
			]
		},
		"commands": {
			"patterns": [
				{
					"name": "keyword.other.cs2-cfg",
					"match": "\\+(?:attack|attack2|back|duck|forward|jump|left|lookatweapon|reload|right|showscores|spray_menu|sprint|use|voicerecord)\\b"
				},
				{
					"name": "keyword.other.cs2-cfg",
					"match": "\\b(?:adsp|ai|alias|ammo|anim|animevents|animgraph|autobuy|axis|battery|benchframe|bind|binddefaults|bindss|bot|budget|buy|c_|callvote|cam|cast|cc|centerview|chat|cl_|clear|clientport|closecaption|connect|clutch|cmd|con_enable|con_log|con_notify|convar|cpu|cs_|csm|cstrike|cvar|demo|developer|dev|disconnect|dota|drop|dsp|echo|editor|endround|engine|ent|eom|exec|exit|ff_|find|fog|fps|func_|g_|game|gl_|gpu|graph|help|host|hud|impulse|incrementvar|info|install|invnext|jointeam|joy|jpeg|key_find|key_list|kick|kill|lastinv|load|log|lobby|map|mat|menu|memory|mm_|mod|model|mouse|mp_|muzzleflash|name|navspace|nav_|net_|noclip|notarget|npc|option|overlay|panorama|particle|pause|perf|phoneme|phys_|ping|play|plugin|print|prop_|quit|r_|radio|rate|record|remove|reset|retry|room_|rope_|rpt|run|safezoney|safezonex|save|say|sc_|screenshot|script|sellbackall|sensitivity|server|setinfo|shake|showtriggers|show_|singlestep|sk_|slot|snd_|sound|soundscape|spec_|spike|startmovie|status|stop|subclass|sv_|sys_|switchhands|take|task|teammenu|teleport|test|thirdperson|timedemo|toggle|tr_|tv_|ui_|unbind|unbindall|user|util|v_|vcollide|vgui|violence|view|voice_|volume|vr_|vs_|weapon_|win_|writecfg|x_|zoom)[_a-zA-Z0-9]*\\b"
				}
			]
		},
		"strings": {
			"patterns": [
				{
					"name": "string.quoted.double.cs2-cfg",
					"begin": "\"",
					"end": "\"",
					"patterns": [
						{ "include": "$self" },
						{ "include": "#commands" },
						{ "include": "#network" },
						{ "include": "#values" },
						{ "include": "#separators" }
					]
				}
			]
		},
		"network": {
			"patterns": [
				{
					"name": "variable.other.cs2-cfg",
					"match": "\\b(25[0-5]|2[0-4][0-9]|[1][0-9][0-9]|[1-9][0-9]|[0-9])(\\.)(25[0-5]|2[0-4][0-9]|[1][0-9][0-9]|[1-9][0-9]|[0-9])(\\.)(25[0-5]|2[0-4][0-9]|[1][0-9][0-9]|[1-9][0-9]|[0-9])(\\.)(25[0-5]|2[0-4][0-9]|[1][0-9][0-9]|[1-9][0-9]|[0-9])(:)(270[0-9]{2})\\b",
					"captures": {
						"2": { "name": "punctuation.separator.cs2-cfg" },
						"4": { "name": "punctuation.separator.cs2-cfg" },
						"6": { "name": "punctuation.separator.cs2-cfg" },
						"8": { "name": "punctuation.separator.cs2-cfg" }
					}
				},
				{
					"name": "variable.other.cs2-cfg",
					"match": "\\b([a-z0-9](?:[a-z0-9-]*[a-z0-9])?)(\\.)([a-z0-9](?:[a-z0-9-]*[a-z0-9])?)(\\.)([a-z]{2,63})(:)(270[0-9]{2})\\b",
					"captures": {
						"2": { "name": "punctuation.separator.cs2-cfg" },
						"4": { "name": "punctuation.separator.cs2-cfg" },
						"6": { "name": "punctuation.separator.cs2-cfg" }
					}
				}
			]
		},
		"values": {
			"patterns": [
				{
					"name": "constant.numeric.cs2-cfg",
					"match": "(?<![\\w.:])[+-]?(?:\\d+\\.\\d+|\\d+|\\.\\d+)(?![\\w.])"
				},
				{
					"name": "constant.language.cs2-cfg",
					"match": "\\b(?:true|false)\\b"
				}
			]
		},
		"separators": {
			"patterns": [
				{
					"name": "punctuation.separator.cs2-cfg",
					"match": "[;|]"
				}
			]
		}
	},
	"scopeName": "source.cs2-cfg"
}
