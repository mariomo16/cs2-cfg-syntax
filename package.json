{
    "name": "cs2-cfg-syntax",
    "displayName": "CS2 Config Syntax",
    "description": "Syntax highlighting for CS2 .cfg files",
    "publisher": "mario-morales-ortega",
    "icon": "./icon.webp",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.104.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "cs2-cfg",
                "aliases": [
                    "CS2 Config",
                    "cs2-cfg"
                ],
                "extensions": [
                    ".cfg"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "cs2-cfg",
                "scopeName": "source.cs2-cfg",
                "path": "./syntaxes/cs2-cfg.tmLanguage.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mariomo16/cs2-cfg-syntax"
    },
    "license": "MIT"
}
